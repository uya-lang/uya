// 控制流示例
fn max(a: i32, b: i32) i32 {
    if a > b {
        return a;
    } else {
        return b;
    }
}

fn factorial(n: i32) i32 {
    var result: i32 = 1;
    var i: i32 = 1;
    while i <= n {
        result = result * i;
        i = i + 1;
    }
    return result;
}

fn main() i32 {
    const m: i32 = max(5, 10);
    const f: i32 = factorial(5);
    return m + f;
}

