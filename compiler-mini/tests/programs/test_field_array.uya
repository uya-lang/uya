// 测试程序：验证字段访问后跟数组访问
// 测试 arena.buffer[index] 这种表达式

struct Test {
    buffer: &byte,
    size: i32,
}

fn test_field_array() i32 {
    // 测试：最简单的结构体初始化
    var test: Test = Test{ buffer: null, size: 100 };
    
    // 测试：字段访问
    const size: i32 = test.size;
    
    return size;
}

fn main() i32 {
    const result: i32 = test_field_array();
    return (result != 100) as i32;
}

