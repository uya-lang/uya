// 测试块注释（规范 uya.md §1）

/* 单行块注释 */

fn main() i32 {
    /* 函数内块注释 */
    const a: i32 = 10; /* 行尾块注释 */
    
    /*
        多行块注释
        可以跨越多行
    */
    const b: i32 = 20;
    
    /* 嵌套块注释 - 级别1
        /* 嵌套块注释 - 级别2 */
        继续级别1
    */
    const c: i32 = 30;
    
    /* 深层嵌套
        /* 级别2
            /* 级别3 */
            继续级别2
        */
        继续级别1
    */
    const d: i32 = 40;
    
    // 混合单行和块注释
    /* 块 */ const e: i32 = 50; // 单行
    
    if a + b + c + d + e == 150 {
        return 0;
    }
    return 1;
}

