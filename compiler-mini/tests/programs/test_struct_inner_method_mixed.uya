// 结构体内定义方法（字段与方法混合）
// 预期：main 返回 0

struct Rect {
  width: i32,
  fn getWidth(self: &Self) i32 {
    return self.width;
  },
  height: i32,
  fn getHeight(self: &Self) i32 {
    return self.height;
  },
  fn area(self: &Self) i32 {
    return self.width * self.height;
  }
}

fn main() i32 {
  const r: Rect = Rect{ width: 3, height: 4 };
  const w: i32 = r.getWidth();
  const h: i32 = r.getHeight();
  const a: i32 = r.area();
  if w != 3 || h != 4 || a != 12 {
    return 1;
  }
  return 0;
}
