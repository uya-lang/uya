// 多参数函数测试
// 测试具有多个参数的函数

fn add_four(a: i32, b: i32, c: i32, d: i32) i32 {
    return a + b + c + d;
}

fn multiply(a: i32, b: i32, c: i32) i32 {
    return a * b * c;
}

fn main() i32 {
    const result1: i32 = add_four(1, 2, 3, 4);
    if result1 != 10 {
        return 1;
    }
    
    const result2: i32 = multiply(2, 3, 4);
    if result2 != 24 {
        return 1;
    }
    
    return 0;
}

