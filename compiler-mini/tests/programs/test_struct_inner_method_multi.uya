// 结构体内定义多个方法
// 预期：main 返回 0

struct Calc {
  acc: i32,
  fn add(self: *Self, x: i32) void {
    self.acc = self.acc + x;
  },
  fn get(self: *Self) i32 {
    return self.acc;
  }
}

fn main() i32 {
  var c: Calc = Calc{ acc: 0 };
  c.add(10);
  c.add(20);
  const n: i32 = c.get();
  if n != 30 {
    return 1;
  }
  return 0;
}
