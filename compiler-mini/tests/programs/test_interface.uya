// 接口：定义、实现、装箱与调用
// 预期：main 返回 15（5 + 10）

interface IAdd {
  fn add(self: *Self, x: i32) i32;
}

struct Counter : IAdd {
  value: i32
}

Counter {
  fn add(self: *Self, x: i32) i32 {
    return self.value + x;
  }
}

fn use_it(a: IAdd) i32 {
  return a.add(10);
}

fn main() i32 {
  const c: Counter = Counter{ value: 5 };
  const n: i32 = use_it(c);
  if n != 15 {
    return 1;
  }
  return 0;
}
