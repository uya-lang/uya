extern printf(fmt: *byte, ...) i32;

fn main() i32 {
    // 测试数组元素赋值
    var arr: [i32; 5];
    arr[0] = 10;
    arr[1] = 20;
    arr[2] = 30;
    arr[3] = 40;
    arr[4] = 50;
    
    // 测试变量索引赋值
    var i: i32 = 0;
    if i >= 0 && i < 5 {
        arr[i] = 100;
        i = 1;
        arr[i] = 200;
        
        // 测试通过指针访问
        const ptr: *i32 = &arr[0];
        printf("arr[0] = %d, arr[1] = %d\n", ptr[0], ptr[1]);
    }
    
    return 0;
}

