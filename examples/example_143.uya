// 泛型函数示例

// 基本泛型函数：比较两个值并返回较大者
fn max<T: Ord>(a: T, b: T) T {
    if a > b {
        return a;
    }
    return b;
}

// 使用示例
// const x: i32 = max<i32>(10, 20);  // 返回 20
// const y: f64 = max<f64>(3.14, 2.71);  // 返回 3.14

// 多约束泛型函数：克隆并比较
fn clone_and_compare<T: Clone + Ord>(a: T, b: T) bool {
    const cloned = a.clone();
    return cloned > b;
}

// 泛型函数：交换两个值
fn swap<T>(a: &T, b: &T) void {
    const temp: T = *a;
    *a = *b;
    *b = temp;
}

