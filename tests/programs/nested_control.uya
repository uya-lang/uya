// 嵌套控制流测试
// 测试嵌套的if/while语句

fn main() i32 {
    var sum: i32 = 0;
    var i: i32 = 0;
    
    while i < 3 {
        var j: i32 = 0;
        while j < 2 {
            if i > j {
                sum = sum + 1;
            } else {
                sum = sum + 2;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    
    // i=0: j=0时sum+2, j=1时sum+2 → sum += 4
    // i=1: j=0时sum+1, j=1时sum+2 → sum += 3
    // i=2: j=0时sum+1, j=1时sum+1 → sum += 2
    // 总共: 4 + 3 + 2 = 9
    if sum == 9 {
        return 0;
    }
    return 1;
}

