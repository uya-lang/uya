// 复杂表达式测试
// 测试嵌套的复杂表达式

fn double(x: i32) i32 {
    return x * 2;
}

fn main() i32 {
    const a: i32 = 5;
    const b: i32 = 10;
    
    // 测试复杂的嵌套表达式
    const result1: i32 = (a + b) * 2 - double(a);
    // (5 + 10) * 2 - 10 = 30 - 10 = 20
    if result1 != 20 {
        return 1;
    }
    
    // 测试函数调用和算术运算组合
    const result2: i32 = double(a) + double(b) * 2;
    // 10 + 20 * 2 = 10 + 40 = 50
    if result2 != 50 {
        return 1;
    }
    
    // 测试比较和逻辑运算组合
    const result3: bool = (a < b) && (b > a) && (a + b == 15);
    if result3 != true {
        return 1;
    }
    
    // 测试一元运算符和算术运算
    const result4: i32 = -a + b - (-b);
    // -5 + 10 - (-10) = -5 + 10 + 10 = 15
    if result4 != 15 {
        return 1;
    }
    
    return 0;
}

