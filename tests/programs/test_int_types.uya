// 测试新增整数类型：i8, i16, i64, u8, u16, u32, u64
fn main() i32 {
    const a: i8 = 42;
    const b: i16 = 1000;
    const c: i64 = 999999;
    const d: u8 = 200;
    const e: u16 = 50000;
    const f: u32 = 3000000000;
    const g: u64 = 4000000000;
    if a != 42 { return 1; }
    if b != 1000 { return 2; }
    if c != 999999 { return 3; }
    if d != 200 { return 4; }
    if e != 50000 { return 5; }
    if g != 4000000000 { return 6; }
    const x: i8 = 1 as i8;
    const y: i32 = x as i32;
    if y != 1 { return 7; }
    return 0;
}
