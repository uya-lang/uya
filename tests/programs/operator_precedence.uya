// 运算符优先级测试
// 测试复杂表达式中的运算符优先级

fn main() i32 {
    const a: i32 = 2;
    const b: i32 = 3;
    const c: i32 = 4;
    
    // 测试算术运算符优先级：* 优先于 +
    const result1: i32 = a + b * c;  // 应该是 2 + (3 * 4) = 14
    if result1 != 14 {
        return 1;
    }
    
    // 测试一元运算符优先级：- 优先于 *
    const result2: i32 = -a * b;  // 应该是 (-2) * 3 = -6
    if result2 != -6 {
        return 1;
    }
    
    // 测试比较运算符优先级：< 优先于 &&
    const result3: bool = a < b && b < c;  // 应该是 (2 < 3) && (3 < 4) = true
    if result3 != true {
        return 1;
    }
    
    // 测试逻辑运算符优先级：&& 优先于 ||
    const result4: bool = false && true || true;  // 应该是 (false && true) || true = true
    if result4 != true {
        return 1;
    }
    
    // 测试括号改变优先级
    const result5: i32 = (a + b) * c;  // 应该是 (2 + 3) * 4 = 20
    if result5 != 20 {
        return 1;
    }
    
    return 0;
}

