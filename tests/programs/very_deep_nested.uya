// 非常深的嵌套测试
// 测试5层嵌套的if语句

fn main() i32 {
    var value: i32 = 0;
    const a: i32 = 1;
    const b: i32 = 2;
    const c: i32 = 3;
    const d: i32 = 4;
    const e: i32 = 5;
    
    if a > 0 {
        if b > 1 {
            if c > 2 {
                if d > 3 {
                    if e > 4 {
                        value = 1000;
                    } else {
                        value = 2000;
                    }
                } else {
                    value = 3000;
                }
            } else {
                value = 4000;
            }
        } else {
            value = 5000;
        }
    } else {
        value = 6000;
    }
    
    // 应该执行 value = 1000
    if value != 1000 {
        return 1;
    }
    
    // 测试另一个路径
    const f: i32 = 0;
    var value2: i32 = 0;
    
    if a > 0 {
        if b > 1 {
            if c > 2 {
                if d > 3 {
                    if f > 4 {
                        value2 = 1000;
                    } else {
                        value2 = 2000;
                    }
                } else {
                    value2 = 3000;
                }
            } else {
                value2 = 4000;
            }
        } else {
            value2 = 5000;
        }
    } else {
        value2 = 6000;
    }
    
    // 应该执行 value2 = 2000
    if value2 != 2000 {
        return 2;
    }
    
    return 0;
}

